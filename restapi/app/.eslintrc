{
  "extends": ["eslint:recommended", "airbnb"],
  "rules": {
    "linebreak-style": [
      "error",
      "windows"
    ],
    "comma-dangle": ["error", {"functions": "never"}],
    "no-underscore-dangle": 0,
    "no-nested-ternary": 0,
    "radix": 0,
    "semi": 0,
    "function-paren-newline": 0,
    "func-names": 0,
    "prefer-arrow-callback": 0,
    "curly": 0
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  }
}